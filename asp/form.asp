<script>
	function selectEmailEvent(selectObj) {
		if (selectObj.value == '1'){		
			document.getElementById("eMail2").value = '';
		} else {
			document.getElementById("eMail2").value = selectObj.value;
		}
	}
	
	function cancel_fn() {
		if(confirm("다시 작성하시겠습니까?") == true) {
			location.href = "/microsurgery/receive/lecture_write.asp"
        }
	}
	
	function write_fn() {
		if(write_form.UserNameK.value == '') { alert('성명을 입력해주세요.'); write_form.UserNameK.focus(); return; }
		if(write_form.Dept.value == '') { alert('소속을 입력해주세요.'); write_form.Dept.focus(); return; }
		if(write_form.eMail1.value == '') { alert('이메일을 입력해주세요.'); write_form.eMail1.focus(); return; }
		if(write_form.eMail2.value == '') { alert('이메일을 입력해주세요.'); write_form.eMail2.focus(); return; }
		if(write_form.TelNo1.value == '') { alert('전화번호를 입력해주세요.'); write_form.TelNo1.focus(); return; }
		if(write_form.TelNo1.value == '') { alert('전화번호를 입력해주세요.'); write_form.TelNo1.focus(); return; }
		if(write_form.TelNo1.value == '') { alert('전화번호를 입력해주세요.'); write_form.TelNo1.focus(); return; }
		if(write_form.PhoneNo1.value == '') { alert('휴대폰번호를 입력해주세요.'); write_form.PhoneNo1.focus(); return; }
		if(write_form.PhoneNo2.value == '') { alert('휴대폰번호를 입력해주세요.'); write_form.PhoneNo2.focus(); return; }
		if(write_form.PhoneNo3.value == '') { alert('휴대폰번호를 입력해주세요.'); write_form.PhoneNo3.focus(); return; }
		if(write_form.pwd.value == '') { alert('비밀번호를 입력해주세요.'); write_form.pwd.focus(); return; }

//		용량체크
		var maxsize = 30 * 1024 * 1024
		if (document.getElementById('ex_filename1').value != '')
		{ var size1 = document.getElementById("ex_filename1").files[0].size; }
		if (document.getElementById('ex_filename2').value != '')
		{ var size2 = document.getElementById("ex_filename2").files[0].size; }
		if (document.getElementById('ex_filename3').value != '')
		{ var size3 = document.getElementById("ex_filename3").files[0].size; }

		if (size1 >= maxsize){alert('강의원고의 파일용량을 30MB 미만으로 업로드 해주세요.'); return; }
		if (size2 >= maxsize){alert('이력서의 파일용량을 30MB 미만으로 업로드 해주세요.'); return; }
		if (size3 >= maxsize){alert('강의슬라이드의 파일용량을 30MB 미만으로 업로드 해주세요.'); return; }

		if(confirm("접수 하시겠습니까?") == true) {
			document.write_form.submit();
		}
	}




	function tel_number_chk() {
		if( ( event.keyCode >=48 && event.keyCode <= 57 ) || ( event.keyCode >=96 && event.keyCode <= 105 ) || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 46 || event.keyCode == 37 || event.keyCode == 39) {
            this.value;
        } else {
            alert("입력 가능합니다.");
            this.value = "";
            this.focus();
		}

	}

    function numcheck(){
        var chk_text = document.querySelectorAll('.numberOnly_chk input')
        for (var i=0; i<chk_text.length; i++) {
            chk_text[i].addEventListener('keyup', tel_number_chk)
        }
    }

    
        




</script>